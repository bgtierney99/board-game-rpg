config_version=5

[application]

config/name="Board Game Rpg"
run/main_scene="res://Scenes/main.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://Scripts/Global/GameManager.gd"
UIManager="*res://Scenes/UI/UIManager.tscn"
DialogueManager="*res://Scripts/Global/DialogueManager.gd"
SaveManager="*res://Scripts/Global/SaveManager.gd"
BattleManager="*res://Scripts/Global/BattleManager.gd"
TradeManager="*res://Scripts/Global/TradeManager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

